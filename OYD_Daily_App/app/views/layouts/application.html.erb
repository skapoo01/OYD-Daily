<!DOCTYPE html>
<html>
  <head>
    <title>OYDDailyApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery-3.3.1.min', 'application', 'data-turbolinks-track': 'reload' %>

  </head>

  <body>
    <div id="user_nav">
      <% if current_user %>
      Logged in as <%= current_user.username %>
      <%= link_to "Log out", log_out_path %>
    <% end %>
    </div>
    <%= yield %>
  </body>

  <%= debug(params) %>
</html>
